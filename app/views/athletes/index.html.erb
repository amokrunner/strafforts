<body class="hold-transition skin-black-light sidebar-mini page-loading">
  <div class="wrapper">

    <%= render 'header' %>
    <%= render 'sidebar' %>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">

      <!-- Content Header (Page header) -->
      <section class="content-header">
        <h1>Overview</h1>
        <ol class="breadcrumb">
          <li>
            <a class="show-overview" href="#">
              <i class="fa fa-home"></i>
              <%= Settings.app.name %>
            </a>
          </li>
          <li class="active">Overview</li>
        </ol>
      </section>

      <!-- Main content -->
      <section class="content" id="main-content">
      </section>
    </div><!-- /.content-wrapper -->

    <%= render 'footer' %>

    <% if @is_current_user %>
      <%= render 'settings' %>
    <% end %>

  </div><!-- ./wrapper -->

  <script>
    // Google Analytics tracking code.
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r;
      i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      },
      i[r].l = 1 * new Date();
      a = s.createElement(o),
      m = s.getElementsByTagName(o)[0];
      a.async = 1;
      a.src = g;
      m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

    ga('create', '<%= ENV['GOOGLE_ANALYTICS_TRACKING_CODE'] %>', 'auto');
    ga('send', 'pageview');
  </script>

  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'athletes/main' %>

</body>
