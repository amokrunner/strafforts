{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "max-line-length": [true, 120],
        "no-namespace": false,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape"],
        "variable-name": [true, "ban-keywords", "check-format"]
    }
}